{
  "name": "wms-desktop",
  "version": "0.1.0",
  "private": true,
  "main": "dist/main/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "concurrently -k -n TSC,ELECTRON \"tsc -w -p tsconfig.json\" \"wait-on dist/main/main.js && electron .\"",
    "start": "electron ."
  },
  "dependencies": {
    "electron": "^31.3.1"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "typescript": "^5.6.2",
    "wait-on": "^8.0.2"
  }
}
