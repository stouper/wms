{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "node": "20.11.1",
      "env": {
        "EXPO_NO_TELEMETRY": "1",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "node": "20.11.1",
      "env": {
        "EXPO_NO_TELEMETRY": "1",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    },
    "production": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "node": "20.11.1",
      "env": {
        "EXPO_NO_TELEMETRY": "1",
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      }
    }
  }
}
